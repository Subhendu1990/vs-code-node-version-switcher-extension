{
  "name": "vs-code-node-version-switcher-extension",
  "displayName": "VS Code Node Version Switcher",
  "description": "Quickly switch Node.js versions using nvm from VSCode",
  "version": "0.0.1",
  "publisher": "subhendu-patra",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onStartupFinished"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nodeVersionSwitcher.selectVersion",
        "title": "Node: Select Node Version"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.19.4",
    "@types/vscode": "^1.80.0",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "node-version-switcher",
    "vs-code-extension",
    "nvm"
  ],
  "author": "Subhendu Patra",
  "license": "ISC"
}
